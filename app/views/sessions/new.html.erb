<div class="user-form">
<div class="form-head">
  <p class="signup"><a href="<%= new_user_url %>">sign up</a></p>
  <h2 class="login">log in</h2>
    
</div>

<form action="/session" method="post" onsubmit="return checkForm();">
  <input type="hidden" name="authenticity_token" value="<%= form_authenticity_token %>">
  <div class="form-group">
    <br>
    <input placeholder="Username" id="user-email" name="user[email]" type="text" class="form-control">
  </div>
  <div class="form-group">
    <input placeholder="Password" id="user-password" name="user[password]" type="password" class="form-control"><br>
  </div>
  <div class="form-group">
    <input type="submit" value="Submit" class="btn btn-primary"><br>
  </div>
</form>
</div>